{
  "files.exclude": {
    ".eslintignore": true,
    // ".eslintrc.json": true,
    ".gitignore": true,
    "node_modules": true,
    "package-lock.json": true,
    "package.json": true,
    "tsconfig.json": true,
    "utils/suppress.js": true
  },
  "[javascript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "[typescript]": { "editor.defaultFormatter": "dbaeumer.vscode-eslint" },
  "code-runner.executorMap": { "typescript": "tsx" },
  "editor.tokenColorCustomizations": { "comments": "#565b6b" },
  "autolink.queries": [
    {
      "linkPattern": "MDN/([^ ]+)",
      "linkText": "https://developer.mozilla.org/en-US/docs/${0}"
    }
  ]
}
